{
  "name": "open-3d-studio",
  "version": "1.0.0",
  "private": true,
  "description": "Cross-platform 3D AIGC studio built with React and Electron",
  "main": "build/electron.js",
  "homepage": "./",
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "@react-three/postprocessing": "^2.19.1",
    "@react-three/rapier": "^1.5.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.160.0",
    "@xyflow/react": "^12.10.0",
    "ai": "^6.0.5",
    "axios": "^1.6.0",
    "camera-controls": "^3.1.0",
    "dexie": "^4.2.1",
    "framer-motion": "^11.0.0",
    "leva": "^0.10.1",
    "postprocessing": "^6.38.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-redux": "^9.0.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "sonner": "^2.0.7",
    "styled-components": "^6.1.0",
    "three": "^0.160.1",
    "three-stdlib": "^2.29.0",
    "troika-three-text": "^0.52.4",
    "uuid": "^9.0.0",
    "web-vitals": "^3.5.0",
    "workbox-window": "^7.4.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^9.0.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "typescript": "^4.9.5",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run copy-electron-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 &&  cross-env ELECTRON_IS_DEV=true npm run electron\"",
    "electron": "electron .",
    "copy-electron-files": "node scripts/copy-electron-files.js",
    "build-electron": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --publish=never",
    "icons": "chmod +x scripts/generate-icons.sh && ./scripts/generate-icons.sh",
    "dist-mac": "npm run build && electron-builder --mac --publish=never",
    "dist-win": "npm run build && electron-builder --win --publish=never",
    "pack-mac": "npm run dist-mac && echo '\nâœ… macOS builds completed successfully!' && echo 'ðŸ“¦ Find your app at:' && echo '   â€¢ dist/Open 3D Studio-x.x.x.dmg (Intel Mac)' && echo '   â€¢ dist/Open 3D Studio-x.x.x-arm64.dmg (Apple Silicon)'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.open3dstudio.app",
    "productName": "Open 3D Studio",
    "directories": {
      "output": "dist",
      "buildResources": "build-assets"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!node_modules/**/*.{md,txt}",
      "!node_modules/**/LICENSE*",
      "!node_modules/**/README*",
      "!node_modules/**/.eslintrc*",
      "!node_modules/**/.gitignore",
      "!node_modules/**/package-lock.json",
      "!node_modules/**/yarn.lock"
    ],
    "mac": {
      "icon": "build-assets/icon.icns",
      "category": "public.app-category.graphics-design",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "identity": null,
      "gatekeeperAssess": false,
      "hardenedRuntime": false,
      "entitlements": null,
      "entitlementsInherit": null
    },
    "win": {
      "icon": "build-assets/icon.ico",
      "target": "nsis"
    },
    "linux": {
      "icon": "build-assets/icon.png",
      "target": "AppImage"
    },
    "compression": "normal",
    "nodeGypRebuild": false,
    "buildVersion": "1.0.0"
  }
}
